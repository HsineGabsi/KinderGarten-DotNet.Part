@model Solution.Web.Models.ClaimModel

@{
    ViewBag.Title = "DetailsFront";
}
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Admin</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/img/favicon.png">
    <!-- Normalize CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/css/normalize.css")>
    <!-- Main CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/css/main.css")>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/css/bootstrap.min.css")>
    <!-- Fontawesome CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/css/all.min.css")>
    <!-- Flaticon CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/fonts/flaticon.css")>
    <!-- Full Calender CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/css/fullcalendar.min.css")>
    <!-- Animate CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/css/animate.min.css")>
    <!-- Custom CSS -->
    <link rel="stylesheet" href=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/style.css")>
    <!-- Modernize js -->
    <script src=@Url.Content("/Content/Front1/www.radiustheme.com/demo/html/psdboss/akkhor/akkhor/js/modernizr-3.6.0.min.js")></script>
</head>
<body>
    <div class="card height-auto">
        <div class="card-body">
            <div class="heading-layout1">
                <div class="item-title">
                    <h3>Claim details</h3>
                </div>
                <div class="dropdown">
                    <a class="dropdown-toggle" href="#" role="button"
                       data-toggle="dropdown" aria-expanded="false">...</a>

                    <div class="dropdown-menu dropdown-menu-right">
                        <a class="dropdown-item" href="@Url.Action("indexFront","Claim")"><i class="fas fa-cogs text-dark-pastel-green"></i>Back to list</a>
                    </div>
                </div>
            </div>
            <div class="single-info-details">

                <div class="item-content">


                    <div class="info-table table-responsive">
                        <table class="table text-nowrap">
                            <tbody>
                                <tr>
                                    <td>  @Html.DisplayNameFor(model => model.Name)</td>
                                    <td class="font-medium text-dark-medium">@Html.DisplayFor(model => model.Name)</td>
                                </tr>
                                <tr>
                                    <td>  @Html.DisplayNameFor(model => model.Description)</td>
                                    <td class="font-medium text-dark-medium">@Html.DisplayFor(model => model.Description)</td>
                                </tr>
                                <tr>
                                    <td>  @Html.DisplayNameFor(model => model.ClaimDate)</td>
                                    <td class="font-medium text-dark-medium">@Html.DisplayFor(model => model.ClaimDate)</td>
                                </tr>
                                <tr>
                                    <td>@Html.DisplayNameFor(model => model.ParentId)</td>
                                    <td class="font-medium text-dark-medium">@Html.DisplayFor(model => model.ParentId)</td>
                                </tr>
                                <tr>
                                    <td>@Html.DisplayNameFor(model => model.ClaimType)</td>
                                    <td class="font-medium text-dark-medium">@Html.DisplayFor(model => model.ClaimType)</td>
                                </tr>
                                <tr>
                                    <td>@Html.DisplayNameFor(model => model.status)</td>
                                    <td class="font-medium text-dark-medium">@Html.DisplayFor(model => model.ClaimType)</td>
                                </tr>

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
